PROGRAM main

VAR
	a : INT;
	b : SINT;
	c : LDINT;
END_VAR

FOR a := 4 TO 12 BY 2 DO
	a := 2 * (4 - 4) + (a + 1 * 0) ** 2 * 1;
END_FOR

b := 2;
WHILE b < 25 DO
	b := b * 5 + 2;
END_WHILE

c := 0;
REPEAT
	c := (c + 1) * 2;
UNTIL c >= 10
END_REPEAT

IF a + b > c THEN
	c := a;
ELSE
	a := b;
END_IF

END_PROGRAM